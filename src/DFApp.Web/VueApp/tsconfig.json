    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "module": "NodeNext"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
